{
  "Serilog": {
    "Using": ["Bp.Logging", "Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": ["FromLogContext", "WithExceptionDetails"]
  },
  "Service":{
    "Name": "bp-seed",
    "Version": "0.0.0"
  },
  "AllowedHosts": "*"
}
